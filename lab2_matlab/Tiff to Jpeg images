clc
clear all



fold = input('Ingrese direcci√≥n del folder','s')
cd(fold)

direct = strcat(fold,'\*.tiff')

d = dir(direct);
N = {d.name};
j = 1;



for i=1:1:length(N)
    f = N{i};
    im = imread(f);
    ind = num2str(i);
    archN = strcat('archivo','',ind)
    imwrite(im,archN,'JPEG');
    
end
